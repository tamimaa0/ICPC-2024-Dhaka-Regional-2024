#include <bits/stdc++.h>
#define ll long long int
#define db double

const int N=1e4+10;
const ll M = 1000000007;
using namespace std;

vector<bool>prime(N , true) ;

vector<ll>p;

void fiill(){

    p.push_back(1LL);

    while(p.size()<=N){
        p.push_back(p.back()+4LL);
    }

}


void seive(){

    prime[1] = false ;
    prime[0] = false;


    for (int i = 2; i*i <= N; i++) {
        if (prime[i]) {


            for (int j = i * i; j <= N; j += i)
                prime[j] = false;
        }
    }




}

void pr_f(vector<ll>p , vector<ll>&p_f ,ll n , int i){

    if(prime[n])
    {
        p_f.push_back(n) ;
        return;
    }

    if(n%p[i] == 0){
        p_f.push_back(p[i]);
        pr_f(p,p_f, n/p[i], i);

    } else
    {
        pr_f(p,p_f, n, i+1);
    }

}


db s_t(db n){

    return min(n, 37500.0) *0.2;

}

db t_t(db n){
    return min(n, 100000.0)*0.4;
}

db l_t(db n){
return n*0.45;
}



db ans (db n){

    db l= 0 ;
    db r = LONG_MAX ;
    db result = 0 ;

    while(l<=r){

        db tax = 0 ;

        db mid = (l+r)/2 ;

        db x = max(mid-100000 , 0.0) ;
        x= x*0.5 ;
        x = min(12500.0,x)*0.4 ;

        tax += x ;

        db taka = mid ;

        taka -= min(taka, 12500.0) ;

        tax += s_t(taka);

        taka -=  min(taka, 37500.0) ;

        tax += t_t(taka) ;

        taka -= min(taka, 100000.0) ;

        tax += l_t(taka);

        taka -= taka ;

        if(tax <=n){
            l = mid +0.000001 ;
            result = mid ;
        }else
            r= mid - 0.000001 ;

        //cout<<mid<<" "<<tax<<" "<<x<<endl;
    }
    return result ;
}

void solve(){

    ll n;
    cin>>n;

    vector<ll>v(n), pre, pp;

    for(int i=0; i<n; i++)
        cin>>v[i] ;
    pre.push_back(0);
    pp.push_back(INT_MAX);

    for(auto u :v){
        pre.push_back(max(pre.back(), u));
    }
    for(int i= n-1; i>=0; i--){
        pp.push_back(min(pp.back(), v[i]));
    }

    reverse(pp.begin(), pp.end());


    //for(auto u: pre)cout<<u<<" ";cout<<endl;
   // for(auto u: pp)cout<<u<<" ";cout<<endl;

    for(int i=1; i<=n; i++){
        if(pre[i]!=pre[i-1] and pre[i]!= pre[i+1] and pp[i]>pre[i]){

            cout<<i<<endl;
            return ;
        }
    }
    cout<<"Humanity is doomed!"<<endl;
}
//13:13



int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);

    //seive();
    //fiill();



    int t = 1;
    cin >> t;
    int i = 1;
    while (t--)
    {
        cout<<"Case "<<i<<": ";

        solve();
        i++;

    }
    return 0;
    }
